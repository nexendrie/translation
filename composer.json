{
    "name": "nexendrie/translation",
    "description": "Easy translating of Nette applications with neon",
    "keywords": [
      "translation", "translating", "localization", "nette", "neon"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/translation/",
        "issues": "https://gitlab.com/nexendrie/translation/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "nette/utils": "^2.4",
        "nette/neon": "^2.4"
    },
    "require-dev": {
        "nette/tester": "2.0.x-dev as v1.7",
        "mrtnzlml/testbench": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "squizlabs/php_codesniffer": "^2.6",
        "nette/di": "^2.4"
    },
    "suggest": {
        "nette/di": "To use TranslationExtension"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Translation\\": "src/"
        },
        "classmap": ["src/exceptions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
